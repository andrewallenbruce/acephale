$x2[[17]]\n[1] \"Q4\" \"Q5\"\n\n$x2[[18]]\n[1] \"S0\"\n\n\n$x3\n$x3[[1]]\n[1] \"012\"\n\n$x3[[2]]\n[1] \"206\" \"235\" \"254\" \"287\"\n\n$x3[[3]]\n[1] \"418\" \"451\" \"462\" \"471\"\n\n$x3[[4]]\n[1] \"519\" \"530\" \"575\" \"589\"\n\n$x3[[5]]\n[1] \"607\" \"616\" \"646\"\n\n$x3[[6]]\n[1] \"736\"\n\n$x3[[7]]\n[1] \"804\"\n\n$x3[[8]]\n[1] \"932\" \"962\" \"964\"\n\n$x3[[9]]\n[1] \"A20\"\n\n$x3[[10]]\n[1] \"C97\"\n\n$x3[[11]]\n[1] \"D74\"\n\n$x3[[12]]\n[1] \"G60\" \"G87\"\n\n$x3[[13]]\n[1] \"J11\" \"J27\" \"J34\"\n\n$x3[[14]]\n[1] \"L01\"\n\n$x3[[15]]\n[1] \"V51\"\n\n\n$x4\n$x4[[1]]\n[1] \"0684\"\n\n$x4[[2]]\n[1] \"0709\"\n\n$x4[[3]]\n[1] \"1034\"\n\n$x4[[4]]\n[1] \"1200\"\n\n$x4[[5]]\n[1] \"2719\" \"2764\"\n\n$x4[[6]]\n[1] \"4385\"\n\n$x4[[7]]\n[1] \"4540\"\n\n$x4[[8]]\n[1] \"5728\"\n\n$x4[[9]]\n[1] \"6960\"\n\n$x4[[10]]\n[1] \"8301\"\n\n$x4[[11]]\n[1] \"8561\"\n\n$x4[[12]]\n[1] \"8834\"\n\n$x4[[13]]\n[1] \"9636\"\n\n$x4[[14]]\n[1] \"9940\"\n\n$x4[[15]]\n[1] \"C944\"\n\n$x4[[16]]\n[1] \"E060\" \"E097\"\n\n$x4[[17]]\n[1] \"E103\"\n\n$x4[[18]]\n[1] \"J173\"\n\n$x4[[19]]\n[1] \"J332\"\n\n$x4[[20]]\n[1] \"L545\"\n\n$x4[[21]]\n[1] \"L661\"\n\n$x4[[22]]\n[1] \"L718\"\n\n$x4[[23]]\n[1] \"S805\"\n\n$x4[[24]]\n[1] \"T452\"\n\n\n$x5\n$x5[[1]]\n[1] \"22556\" \"24999\"\n\n$x5[[2]]\n[1] \"57240\"\n\n$x5[[3]]\n[1] \"61055\" \"61330\" \"65175\" \"68801\" \"69666\"\n\n$x5[[4]]\n[1] \"74177\" \"75743\" \"77048\"\n\n$x5[[5]]\n[1] \"82693\" \"82747\" \"88364\" \"88375\"\n\n$x5[[6]]\n[1] \"97124\" \"97814\"\n\n$x5[[7]]\n[1] \"E0575\"\n\n$x5[[8]]\n[1] \"G2184\" \"G8664\" \"G8696\" \"G8842\"\n\n$x5[[9]]\n[1] \"J1071\" \"J1572\" \"J7649\"\n\n$x5[[10]]\n[1] \"L5985\"\n\n$x5[[11]]\n[1] \"T1019\" \"T1025\" \"T2007\" \"T2024\" \"T2036\"\n\n$x5[[12]]\n[1] \"1505F\"\n\n$x5[[13]]\n[1] \"0329T\" \"0764T\" \"0882T\"\n\n$x5[[14]]\n[1] \"0199U\"\n```\n\n\n:::\n:::\n\n\n\n\n### Letters\n\n\n\n\n::: {.cell layout-align=\"center\"}\n::: {.cell-output .cell-output-stderr}\n\n```\nHCPCS Ending with Letter\n```\n\n\n:::\n\n::: {.cell-output .cell-output-stdout}\n\n```\nA : 33\nF : 565\nM : 13\nT : 430\nU : 337\n```\n\n\n:::\n\n::: {.cell-output .cell-output-stderr}\n\n```\nTotal: 1378\n```\n\n\n:::\n\n::: {.cell-output .cell-output-stderr}\n\n```\nHCPCS Beginning with Letter\n```\n\n\n:::\n\n::: {.cell-output .cell-output-stdout}\n\n```\nA : 793\nB : 51\nC : 548\nE : 639\nG : 1939\nH : 89\nJ : 1068\nK : 175\nL : 913\nM : 360\nP : 57\nQ : 462\nR : 3\nS : 500\nT : 113\nU : 5\nV : 203\n```\n\n\n:::\n\n::: {.cell-output .cell-output-stderr}\n\n```\nTotal:7918\n```\n\n\n:::\n:::\n\n\n\n\n# Grouping\n\nGroup each set of vectors into regex patterns. Deal with groups containing letters separately.\n\n::: {.panel-tabset}\n\n## Group 3\n\n\n\n\n::: {.cell layout-align=\"center\"}\n\n```{.r .cell-code}\nnot_na <- \\(x) !cheapr::is_na(x)\n\ngroup_3 <- function(x) {\n\n  x <- collapse::get_elem(x, \"x3\")\n  \n  if (empty(x)) return(x)\n  \n  purrr::map(x, function(x) {\n    \n    idx <- data.table::data.table(\n      code = x,\n      grp = sf_sub(x, 1, 2),\n      a1 = take_at(x),\n      a2 = take_at(x, 2),\n      a3 = take_at(x, 3),\n      i1 = collapse::groupid(take_at(x))) |>\n      collapse::fgroup_by(a1) |>\n      collapse::fmutate(i2 = collapse::groupid(a2)) |>\n      collapse::fgroup_by(a1, a2) |>\n      collapse::fmutate(i3 = collapse::groupid(a3)) |> \n      collapse::fungroup()\n      \n      lone <- collapse::fcount(idx, i1, add = TRUE) |>\n        collapse::fsubset((i2 + i3 + N) == 3)\n      \n      last <- collapse::fsubset(idx, code %!in% lone[[\"code\"]])\n      \n      last <- collapse::join(\n        collapse::fcount(last, a1, a2),\n        collapse::fcount(last, a1, name = \"G\"), on = \"a1\", verbose = 0) |>\n        collapse::fsubset(N == G) |>\n        collapse::join(last, on = c(\"a1\", \"a2\"), how = \"right\", verbose = 0) |>\n        collapse::fsubset(not_na(N))\n      \n      rest <- collapse::fsubset(idx, code %!in% c(lone[[\"code\"]], last[[\"code\"]]))\n      \n      c(if (empty(lone)) NULL else list(lone[[\"code\"]]),\n        if (empty(last)) NULL else gchop(last[[\"code\"]], last[[\"a1\"]]),\n        if (empty(rest)) NULL else gchop(rest[[\"code\"]], rest[[\"grp\"]]))\n    })\n}\n\nc(\"C75\", \"C97\", \"G02\", \"G04\") |> \n  split_lengths() |> \n  remove_redundant() |>\n  split_first() |>\n  group_3()\n```\n\n::: {.cell-output .cell-output-stdout}\n\n```\n[[1]]\n[[1]][[1]]\n[1] \"C75\"\n\n[[1]][[2]]\n[1] \"C97\"\n\n\n[[2]]\n[[2]][[1]]\n[1] \"G02\" \"G04\"\n```\n\n\n:::\n\n```{.r .cell-code}\nlong_test2[nchar(long_test2) == 3] |> \n  split_lengths() |> \n  remove_redundant() |>\n  split_first() |>\n  group_3()\n```\n\n::: {.cell-output .cell-output-stdout}\n\n```\n[[1]]\n[[1]][[1]]\n[1] \"021\" \"022\" \"026\" \"027\"\n\n[[1]][[2]]\n[1] \"032\" \"033\"\n\n[[1]][[3]]\n[1] \"042\" \"044\"\n\n[[1]][[4]]\n[1] \"050\"\n\n\n[[2]]\n[[2]][[1]]\n[1] \"157\"\n\n\n[[3]]\n[[3]][[1]]\n[1] \"209\"\n\n[[3]][[2]]\n[1] \"225\"\n\n[[3]][[3]]\n[1] \"271\"\n\n\n[[4]]\n[[4]][[1]]\n[1] \"308\"\n\n[[4]][[2]]\n[1] \"312\"\n\n[[4]][[3]]\n[1] \"326\"\n\n[[4]][[4]]\n[1] \"347\" \"348\"\n\n[[4]][[5]]\n[1] \"355\"\n\n[[4]][[6]]\n[1] \"362\"\n\n\n[[5]]\n[[5]][[1]]\n[1] \"503\" \"505\"\n\n[[5]][[2]]\n[1] \"544\"\n\n[[5]][[3]]\n[1] \"552\" \"553\"\n\n[[5]][[4]]\n[1] \"585\" \"586\" \"587\" \"588\" \"589\"\n\n\n[[6]]\n[[6]][[1]]\n[1] \"610\" \"612\"\n\n[[6]][[2]]\n[1] \"630\"\n\n[[6]][[3]]\n[1] \"644\" \"646\"\n\n\n[[7]]\n[[7]][[1]]\n[1] \"765\"\n\n\n[[8]]\n[[8]][[1]]\n[1] \"920\" \"921\" \"922\"\n\n[[8]][[2]]\n[1] \"958\"\n\n\n[[9]]\n[[9]][[1]]\n[1] \"C75\"\n\n[[9]][[2]]\n[1] \"C97\"\n\n\n[[10]]\n[[10]][[1]]\n[1] \"E06\"\n\n\n[[11]]\n[[11]][[1]]\n[1] \"G02\" \"G04\"\n\n\n[[12]]\n[[12]][[1]]\n[1] \"S23\"\n```\n\n\n:::\n\n```{.r .cell-code}\nrandom_hcpcs(50) |> \n  split_lengths() |> \n  remove_redundant() |>\n  split_first() |>\n  group_3()\n```\n\n::: {.cell-output .cell-output-stdout}\n\n```\n[[1]]\n[[1]][[1]]\n[1] \"234\" \"235\"\n\n[[1]